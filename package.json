{
  "name": "sphere-node-sdk",
  "description": "Officially supported Node.js SDK library for working with the SPHERE.IO HTTP API, with OAuth2 support.",
  "version": "1.3.3",
  "homepage": "https://github.com/sphereio/sphere-node-sdk",
  "author": {
    "name": "Nicola Molinari",
    "email": "nicola.molinari@commercetools.de"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/sphereio/sphere-node-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/sphereio/sphere-node-sdk/issues"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/sphereio/sphere-node-sdk/blob/master/LICENSE-MIT"
    }
  ],
  "main": "./dist/lib",
  "engines": {
    "node": ">= 0.10.0"
  },
  "scripts": {
    "clean": "rm -rf dist && mkdir -p dist",
    "build": "npm run clean && babel lib --out-dir dist/lib",
    "build:test": "npm run build && babel test --out-dir dist/test",
    "jasmine": "JASMINE_CONFIG_PATH=test/jasmine.config.json jasmine",
    "test": "npm run build:test && npm run jasmine",
    "test:watch": "nodemon",
    "preinstall": "npm run build"
  },
  "dependencies": {
    "babel": "5.1.11",
    "babel-runtime": "5.1.11",
    "node-fetch": "1.1.0"
  },
  "devDependencies": {
    "babel-core": "5.1.11",
    "babel-eslint": "3.0.1",
    "jasmine": "2.2.1",
    "nodemon": "1.3.7"
  },
  "keywords": [
    "sphere",
    "sphereio",
    "rest",
    "api",
    "sdk",
    "sync",
    "promise",
    "ecommerce"
  ]
}
